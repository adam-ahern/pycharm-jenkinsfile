<filetype binary="false" description="Jenkinsfile/Groovy Support" name="Jenkinsfile">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="//" />
      <option name="COMMENT_START" value="/*" />
      <option name="COMMENT_END" value="*/" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
      <option name="LINE_COMMENT_AT_START" value="true" />
    </options>
    <keywords keywords="&quot;&quot;&quot;;def;else;if;import;return" ignore_case="true" />
    <keywords2 keywords="field;library;node;parallel;stage;string;pipeline;agent;environment;steps;stages;step;withCredentials;script;post;always;success;failure;aborted;unstable;parameters" />
    <keywords3 keywords="print;sh" />
    <keywords4 keywords="catch;finally;try" />
  </highlighting>
  <extensionMap>
    <mapping pattern="Jenkinsfile" />
    <mapping pattern="Jenkinsfile*" />
    <mapping pattern="*.groovy" />
  </extensionMap>
</filetype>
